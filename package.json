{
  "name": "mmo-ecc-fe",
  "version": "1.10.0",
  "description": "Export catch certificate front end",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "~14.18.1",
    "npm": "~6.14.4"
  },
  "scripts": {
    "start": "npm run dev:start",
    "start-with-idm": "npm run dev:start-with-idm",
    "dev:start": "npm run dev:start-without-idm",
    "dev:start-with-idm": "npm-run-all --parallel dev:build-server dev:build-client && cross-env REDIS_HOST_NAME=127.0.0.1 REDIS_HOST_PORT=6379 APP_USES_LOCAL_IDM=true IDENTITY_APPDOMAIN=http://localhost:3001 PORT=3001 APP_USES_LOCAL_DYM=true APP_USES_HTTPS=false npm-run-all --parallel dev:start-server-with-idm dev:build-server-watch dev:build-client-watch",
    "dev:start-without-idm": "npm-run-all --parallel dev:build-server dev:build-client && cross-env PORT=3001 DISABLE_IDM=true npm-run-all --parallel dev:start-server-without-idm dev:build-server-watch dev:build-client-watch",
    "debug:start-without-idm": "npm-run-all --parallel dev:build-server dev:build-client && cross-env PORT=3001 DISABLE_IDM=true npm-run-all --parallel debug:start-server-without-idm dev:build-server-watch dev:build-client-watch",
    "dev:start-server-without-idm": "cross-env PORT=3001 nodemon build/bundle.js",
    "debug:start-server-without-idm": "cross-env PORT=3001 node --inspect build/bundle.js",
    "dev:start-server-with-idm": "cross-env IDENTITY_APPDOMAIN=http://localhost:3001 PORT=3001 APP_USES_HTTPS=false APP_USES_LOCAL_IDM=true APP_USES_LOCAL_DYM=true nodemon build/bundle.js",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.js",
    "dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.client.js",
    "dev:build-server-watch": "cross-env NODE_ENV=development webpack --config webpack.server.js --watch",
    "dev:build-client-watch": "cross-env NODE_ENV=development webpack --config webpack.client.js --watch",
    "prod:build-server": "cross-env NODE_ENV=production webpack --config webpack.server.prod.js",
    "prod:build-client": "cross-env NODE_ENV=production webpack --config webpack.client.prod.js",
    "test": "npm run test:unit",
    "test:unit": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=test-results.xml NODE_ENV=test jest test/unit/ --coverage --ci",
    "test:ci": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=junit.xml NODE_ENV=test jest test/unit/ --runInBand --watchAll=false --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --ci",
    "test-vstack": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=test-results.xml NODE_ENV=test jest test/unit/ --coverage --silent --ci",
    "tdd": "npm run test:unit -- --watch",
    "lint": "eslint . --ext .js",
    "stylelint": "stylelint scss/**/*.scss",
    "docker:build-all": "npm-run-all --parallel prod:build-server prod:build-client",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "extract": "i18next --config src/client/i18next-parser.config.js"
  },
  "author": "",
  "dependencies": {
    "@azure/event-hubs": "^2.1.4",
