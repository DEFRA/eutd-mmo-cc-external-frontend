    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "5.1.2",
    "webpack": "5.37.1",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "^4.7.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/client/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/test/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/client/reducers/index.js",
      "<rootDir>/src/client/components/elements/SecondaryButton.js",
      "<rootDir>/src/client/components/GovukVesselsAutocomplete.js",
      "<rootDir>/src/client/client.js",
      "<rootDir>/src/client/Routes.js",
      "<rootDir>/src/client/App.js",
      "<rootDir>/src/client/routes/Transport.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 77,
        "functions": 84,
        "lines": 85,
        "statements": 85
      }
    },
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm test --watchAll=false --findRelatedTests --bail"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
